# Anthropic API Key (get from https://console.anthropic.com/)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# OpenAI API Key (for embeddings)
OPENAI_API_KEY=your_openai_api_key_here

# Supabase Configuration
SUPABASE_URL=your_supabase_project_url
SUPABASE_SERVICE_KEY=your_supabase_service_role_key

# PostgreSQL Connection (alternative to Supabase client)
DATABASE_URL=postgresql://user:password@host:port/database

# MCP Server Configuration
MCP_SERVER_PORT=3000
MCP_SERVER_HOST=localhost

# floatctl Integration (optional - defaults to 'floatctl' in PATH)
# Set this to the path of your floatctl binary for best performance
# If not set, assumes 'floatctl' is in PATH (install with: cargo install --path floatctl-cli --features embed)
FLOATCTL_BIN=floatctl
# Example: FLOATCTL_BIN=/Users/evan/.cargo/bin/floatctl
